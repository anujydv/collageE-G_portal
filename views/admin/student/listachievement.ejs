<% include ../partials/header.ejs %>
<div class="wrapper">
    <% include ../partials/navigation.ejs %>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-xs-12 col-sm-12">
                <div class="box">
                    <% if(msg){ %>
                    <% if(status === true){ %>
                    <div class="alert alert-success col-md-6 col-md-offset-3">
                        <%}%>
                                    <% if(status === false){ %>
                        <div class="alert alert-danger col-md-6 col-md-offset-3">
                            <%}%>
                                        <p><%= msg%></p>
                        </div>
                        <%}%>
                    <div class="box-header">
                        <h3 class="box-title">Achievement Table</h3>
                        <div class="box-tools">
                            <div class="input-group input-group-sm" style="width: 150px;">
                                <input type="text" name="table_search" class="form-control pull-right"
                                    placeholder="Search">

                                <div class="input-group-btn">
                                    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body table-responsive no-padding">
                        <table class="table table-hover">
                            <tbody>
                                <tr>
                                    <th>Title</th>
                                    <th>Event Collage</th>
                                    <th>Event Name</th>
                                    <th>Event Year</th>
                                    <th>Position</th>
                                </tr>
                                <% if(achivement_data){%>
                                <% for(achivement of achivement_data){%>
                                <tr>
                                    <td><%= achivement.achiv_heading%></td>
                                    <td><%= achivement.event_college%></td>
                                    <td><%= achivement.event_name%></td>
                                    <td><%= achivement.event_year%></td>
                                    <td><%= achivement.position%></td>
                                </tr>
                                <% }}else{%>
                                    <tr>
                                        <p>No Achievement Added</p>
                                    </tr>
                                <% }%>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<footer class="main-footer">
    <div class="pull-right hidden-xs">
        <b>Version</b> 1.0.0
    </div>
